<header>
    {{#with heading}}
    <div class="card">
        <div class="card-body">
            {{#with main}}
                <h1>{{title}}</h1>
                <p>{{text}}</p>

                {{#with button_text}}
                <button class="btn btn-primary">{{solid}}</button>
                
                {{> space}}

                <button class="btn btn-outline-primary">{{outline}}</button>
                {{/with}}
            {{/with}}
        </div>
    </div>

    {{> space big=true}}

    <div class="card">
        <div class="card-body">
            {{#with second}}
                <h2>{{title}}</h2>
                <p>{{text}}</p>


                {{#with button_text}}
                    <button class="btn btn-secondary">{{solid}}</button>
                    {{> space}}
                    <button class="btn btn-outline-secondary">{{outline}}</button>
                {{/with}}
            {{/with}}
        </div>
    </div>

    {{> space big=true}}

    <div class='card'>
        <div class="card-body">
            {{#with subblock}}
                <h3>{{title}}</h3>
                <p>{{text}}</p>

                {{#with button_text}}
                    <button class="btn btn-dark">{{solid}}</button>

                    {{> space}}

                    <button class="btn btn-outline-dark">{{outline}}</button>
                {{/with}}
            {{/with}}
        </div>
    </div>
{{/with}}
</header>

    {{> space}}
    <hr>

<main>
    {{#with nested_display}}
    <div class="card">
        <div class="card-body">
            <div class="display-1">{{title}}</div>
            <br>
            <div class="card">
                <div class="card-body">
                    {{#with children_display}}
                        <div class="display-4">{{title}}</div>
                        <br>
                        <p>{{text}}</p>

                        <br>

                        <button class="btn btn-warning">{{button.text}}</button>

                    {{/with}}
                </div>
            </div>

            <br>

            <h6>{{header_text}}</h6>
            <br>
            <button class="btn btn-info">{{button.text}}</button>
        </div>
    </div>
    {{/with}}

    {{> space big=true}}


    <div class="card">
        <div class="card-body">
            {{#with paragraph}}
                <p class="text-uppercase">{{title}}</p>

                <p class="text-lowercase">{{text}}</p>

                {{#with button}}
                    <a class="btn btn-link" href="{{url}}">{{text}}</a>
                {{/with}}

                {{> space}}

                <div class="card">
                    <div class="card-name">
                        {{#each list_name}}
                            <ul>
                                <li>
                                    <a class="btn btn-link" href="{{url}}">{{text}}</a>
                                </li>
                            </ul>
                        {{/each}}
                    </div>
                </div>

            {{/with}}
        </div>
    </div>
</main>