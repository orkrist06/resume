<header>
    {{#with page_header}}
        <h1 class="text-center">{{title}}</h1>
        <p class="text-center">{{description}}</p>
    {{/with}}
    
</header>


<main>
    {{#with section}}
        <div class="card">
            <div class="card-body">
                {{#with about_us}}
                    {{#with title_list}}
                    <div class="display-6" "text-end">{{sub}}</div>
                    <br>
                    <h2 class="text-end">{{main}}</h2>
                    {{/with}}

                    <br>

                    <button class="btn btn-outline-info">{{button.text}}</button>
                {{/with}}
            </div>
        </div>

        {{> space}}

        <div class="card">
            <div class="card-body">
                {{#with advantage}}

                <div class="display-5" "text-muted">{{title}}</div>

                <br>
                <br>

                <div class="card">
                    <div class="card-body">
                        {{#with efficiency}}
                            <div class="display-4" "text-muted">{{title}}</div>
                            <br>
                            <h6>{{sub_title}}</h6>
                            <br>

                            <p class="text-muted">{{description}}</p>

                            <br>

                            <button class="btn btn-outline-warning">{{button}}</button>
                        {{/with}}
                    </div>
                </div>

                <div class="card">
                    <div class="card-body">
                        {{#with innovation}}
                        <div class="text-center">
                            <span class="display-4" "text-muted">{{title}}</span>
                        </div>
                        <br>
                        <h6 class="text-center">{{sub_title}}</h6>
                        <br>
                        <p class="text-center" "text-muted">{{description}}</p>
                        <br>

                        <button class="btn btn-outline-warning">{{button}}</button>
                        {{/with}}
                    </div>
                </div>

                <div class="card">
                    <div class="card-body">
                        {{#with collaboration}}
                        <div class="text-end" >
                            <span class="display-4">{{title}}</span>
                        </div>
                        <br>
                        <h6 class="text-end">{{sub_title}}</h6>
                        <br>
                        <p class="text-end" "text-muted ">{{description}}</p>
                        <br>
 
                        <button class="btn btn-outline-warning">{{button}}</button>
                        {{/with}}
                    </div>
                </div>
                
                {{/with}}
            </div>
        </div>

        <div class="card">
            <div class="card-body">
                {{#with agreement}}
                    <div class="display-5">{{title}}</div>
                    <br>
                    <div class="card">
                        <div class="card-body">
                            {{#with info}}
                                <h5>{{sub_title}}</h5>
                                <br>
                                <div class="card">
                                    <div class="card-body">
                                        {{#with detail}}
                                            <p>{{text}}</p>

                                            <br>
                                            <button class="btn btn-success">{{button}}</button>
                                        {{/with}}
                                    </div>
                                </div>

                            {{/with}}
                        </div>
                    </div>
                {{/with}}
            </div>
        </div>
    {{/with}}
</main>